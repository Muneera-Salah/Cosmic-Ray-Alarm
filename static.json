{
    "root": "public",
    "root": "dist/",
    "canonical_host": "https://cosmic-ray-alarm.herokuapp.com/",
    "encoding": "US-ASCII",
    "clean_urls": true,
    "logging": {
        "access": false,
        "error": "warn"
    },
    "routes": {
        "/*.html": "index.html",
        "/route/**": "bar/baz.html"
    },
    "routes": {
        "/assets/*": "/assets/",
        "/**": "index.html"
    },
    "redirects": {
        "/old/gone/": {
            "url": "/",
            "status": 302
        }
    },
    "https_only": true,
    "basic_auth": true,
    "proxies": {
        "/api/": {
            "origin": "https://hone-ember-todo-rails.herokuapp.com/"
        }
    },
    "headers": {
        "/": {
            "Cache-Control": "no-store, no-cache"
        },
        "/assets/**": {
            "Cache-Control": "public, max-age=512000"
        },
        "/assets/webfonts/*": {
            "Access-Control-Allow-Origin": "*"
        }
    },
    "headers": {
        "/**": {
            "Access-Control-Allow-Origin": "*"
        }
    },
    "headers": {
        "/**": {
            "X-Foo": "bar",
            "X-Bar": "baz"
        },
        "/foo": {
            "X-Foo": "foo"
        }
    }
}